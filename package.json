    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "object-assign": "^4.1.1",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "rollup": "^2.33.3",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "serve": "^11.3.2",
    "styled-components": "^5.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "node_modules/@solidoc/iffe-cli/configs/cz-customizable.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --config node_modules/@solidoc/iffe-cli/configs/commitlint.js --env HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./packages/**/src/**": "yarn fix"
  },
  "prettier": "./configs/prettierrc.js",
  "release": {
    "extends": "./configs/releaserc.json"
  }
}
