{
  "name": "iffe-monorepos",
  "version": "0.0.0-development",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "homepage": "http://121.89.222.91:3000/",
  "repository": {
    "type": "git",
    "url": "https://github.com/solidoc/iffe-editor.git"
  },
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "test": "jest --config ./scripts/jest.config.js",
    "testw": "yarn test --watch",
    "test-cov": "yarn test --coverage",
    "dev": "node scripts/build.js --watch --env dev --endshell 'yarn rstart'",
    "prod": "node scripts/build.js --env prod",
    "build": "yarn prod",
    "ci-build": "node scripts/ci-build.js",
    "start-react": "yarn dev -p slate-react",
    "start": "yarn rstart",
    "rstart": "rm -f tsconfig.json & PORT=1234 yarn react-scripts start",
    "rbuild": "yarn react-scripts build",
    "rs-test": "yarn react-scripts test",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint \"./packages/**/src/**\" --ext .ts,.tsx -c ./configs/eslintrc.js",
    "lint:prettier": "prettier --list-different \"./packages/**/src/*.{css,js,jsx,ts,tsx}\"",
    "commit": "git add . && git-cz",
    "push": "git push",
    "pull": "git pull --reb",
    "up": "yarn upgrade-interactive --latest",
    "ps": "node scripts/publish.js",
    "git-setting": "git config core.ignorecase false",
    "npm:publish": "node scripts/npm-publish.js",
    "clean": "node scripts/dist-clean.js",
    "i": "lerna bootstrap & export NODE_OPTIONS='--max_old_space_size=8096' & yarn build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
