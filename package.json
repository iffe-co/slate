    "@types/material-ui": "^0.21.7",
    "@types/esrever": "^0.2.0",
    "@types/mathjax": "^0.0.36",
    "@types/md5": "^2.2.1",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.14.9",
